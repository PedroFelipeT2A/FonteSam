                            <Type>Integer</Type>
                    </Property>
                    <Property>
                            <Name>StreamMonitorLogging</Name>
                            <Value>false</Value>
                            <Type>Boolean</Type>
                    </Property>
                    <Property>
                            <Name>limitStreamViewersMaxViewers</Name>
                            <Value>ESPECTADORES</Value>
                            <Type>Integer</Type>
                    </Property>
			         <Property>
			         	     <Name>securityPlayMaximumConnections</Name>
			         	     <Value>ESPECTADORES</Value>
			         	     <Type>Integer</Type>
			         </Property>
                    <!-- CC Secure Token PlaceHolder -->
                    <Property>
                            <Name>requireSecureConnection</Name>
                            <Value>false</Value>
                            <Type>Boolean</Type>
                    </Property>
                    <Property>
                            <Name>aliasMapFileStream</Name>
                            <Value>${com.wowza.wms.context.VHostConfigHome}/conf/${com.wowza.wms.context.Application}/aliasmap.stream.txt</Value>
                    </Property>
                    <Property>
                            <Name>aliasMapFilePlay</Name>
                            <Value>${com.wowza.wms.context.VHostConfigHome}/conf/${com.wowza.wms.context.Application}/aliasmap.play.txt</Value>
                    </Property>
                    <Property>
                            <Name>aliasMapPathDelimiter</Name>
                            <Value>/</Value>
                    </Property>
                    <Property>
